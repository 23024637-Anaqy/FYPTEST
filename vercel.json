{
  "version": 2,
  "functions": {
    "api/*.js": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/api/debug-login",
      "dest": "/api/debug-login.js"
    },
    {
      "src": "/api/debug-verbose", 
      "dest": "/api/debug-verbose.js"
    },
    {
      "src": "/api/logs-test",
      "dest": "/api/logs-test.js"
    },
    {
      "src": "/api/path-test",
      "dest": "/api/path-test.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
